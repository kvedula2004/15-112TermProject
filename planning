Point {
    attributes {
        x coordinate
        y coordinate
        label
        label x and y coordinate
        can move (bool)
        object points is on
        currently drawn (or hidden)
    }
}

Line {
    attributes {
        label
        label x and y coordinate
        slope
        point on line
        currently drawn (or hidden)
    }
}

